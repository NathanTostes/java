package bankSystem;

public class Manager extends Enployee {
	private String password;
	
	public Manager (String name, String cpf, double salary, String password) {
		super(name, cpf, salary);
		this.password = password;
	}
	
	public void autentification (String password) {
		if (this.password == password) {
			System.out.println("Sucefful access");
		} else {
			System.out.println("Error in acces");
		}
	}
	
	@Override
	public double getSalaryBonus() {
		return salary * 0.15;
	}

	@Override
	public String toString() {
		return "Manager [password=" + password + "]";
	}
}
