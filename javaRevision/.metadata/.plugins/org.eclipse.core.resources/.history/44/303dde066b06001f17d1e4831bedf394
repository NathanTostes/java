package bankSystem;

public class Enployee {
	private String name;
	private String cpf;
	private double salary;
	private float salaryBonus;

	public Enployee (String name, String cpf, double salary) {
		super();
		this.name = name;
		this.cpf = cpf;
		this.salary = salary;
	}
	
	public 

	@Override
	public String toString() {
		return "Enployee [name=" + name + ", cpf=" + cpf + ", salary=" + salary + "]";
	}
}
