package bankSystem;

public class Manager implements Authenticable {
	private String password;

	public Manager(String name, String cpf, double salary, String password) {
		super(name, cpf, salary);
		this.password = password;
	}

	@Override
	public boolean autentification(String password) {
		if (this.password == password) {
			return true;
		} else {
			return false;
		}
	}

	@Override
	public double getSalaryBonus() {
		return (this.salary * 0.1) + 1000;
	}

	@Override
	public String toString() {
		return super.toString() + " password: " + this.password + "]";
	}
}
